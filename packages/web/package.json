{
  "name": "@evolu/web",
  "version": "2.0.0-preview.1",
  "description": "Evolu for web",
  "keywords": [
    "evolu",
    "web"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "browser": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "shx rm -rf dist && tsc",
    "clean": "shx rm -rf .turbo node_modules dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "3.50.0-build1"
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "@types/web-locks-api": "^0.0.5",
    "typescript": "^5.8.3",
    "user-agent-data-types": "^0.4.2",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "@evolu/common": "^7.0.0-preview.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": []
}
