{
  "name": "@evolu/react-native",
  "version": "11.1.2",
  "description": "Evolu for React Native",
  "keywords": [
    "evolu",
    "react-native",
    "react-hooks"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/index.native.d.ts",
  "exports": {
    "react-native": "./dist/index.native.js",
    "default": "./dist/index.web.js"
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf .turbo node_modules dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "expo-updates": "^0.25.11"
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/common-react": "workspace:*",
    "@evolu/react": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "eslint-config-evolu": "workspace:*",
    "expo": "^51.0.5",
    "expo-sqlite": "^14.0.3",
    "react-native": "^0.74.1",
    "typescript": "^5.4.5",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@evolu/common-react": "^8.1.2",
    "expo": "^51.0.0",
    "expo-sqlite": "^14.0.3",
    "react-native": "^0.74.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": []
}
