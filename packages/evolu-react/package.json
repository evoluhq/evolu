{
  "name": "@evolu/react",
  "version": "1.0.2",
  "description": "Evolu for React",
  "keywords": [
    "evolu",
    "react",
    "react-hooks"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "browser": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "lint": "TIMING=1 eslint src --ext .ts,.tsx",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/common-react": "workspace:*",
    "@evolu/common-web": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "@types/react-dom": "^18.2.8",
    "eslint": "^8.50.0",
    "eslint-config-evolu": "workspace:*",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@evolu/common": "^1.0.2",
    "@evolu/common-react": "^1.0.2",
    "@evolu/common-web": "^1.0.1",
    "@types/react-dom": "^18.2.7",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.16"
  },
  "sideEffects": false
}
