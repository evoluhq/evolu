{
  "name": "evolu",
  "version": "8.2.0",
  "description": "React Hooks library for local-first apps with end-to-end encrypted backup and sync using SQLite and CRDT.",
  "keywords": [
    "CRDT",
    "E2EE",
    "evolu",
    "local-first",
    "offline",
    "react-hooks",
    "react-native",
    "react",
    "SQLite"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "browser": "./dist/src/index.js"
    },
    "./Timestamp": {
      "types": "./dist/src/Timestamp.d.ts",
      "import": "./dist/src/Timestamp.js"
    },
    "./MerkleTree": {
      "types": "./dist/src/MerkleTree.d.ts",
      "import": "./dist/src/MerkleTree.js"
    },
    "./Protobuf": {
      "types": "./dist/src/Protobuf.d.ts",
      "import": "./dist/src/Protobuf.js"
    }
  },
  "react-native": {
    "./index.js": "./dist/src/index.native.js"
  },
  "files": [
    "dist/src/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist ./README.md && tsc && cp ../../README.md ./",
    "lint": "TIMING=1 eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\"",
    "proto": "pnpm protoc --ts_out ./src --proto_path protobuf protobuf/Protobuf.proto --ts_opt eslint_disable && pnpm format"
  },
  "dependencies": {
    "@noble/ciphers": "^0.3.0",
    "@noble/hashes": "^1.3.2",
    "@protobuf-ts/runtime": "^2.9.1",
    "@react-native-community/netinfo": "9.4.1",
    "@scure/bip39": "^1.2.1",
    "expo-updates": "^0.18.13",
    "kysely": "^0.26.3",
    "nanoid": "^5.0.1"
  },
  "devDependencies": {
    "@effect/schema": "^0.36.5",
    "@evolu/tsconfig": "workspace:*",
    "@protobuf-ts/plugin": "^2.9.1",
    "@protobuf-ts/protoc": "^2.9.1",
    "@sqlite.org/sqlite-wasm": "3.43.1-build1",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/web-locks-api": "^0.0.2",
    "array-shuffle": "^3.0.0",
    "effect": "2.0.0-next.34",
    "eslint": "^8.50.0",
    "eslint-config-evolu": "workspace:*",
    "expo": "^49.0.11",
    "expo-sqlite": "~11.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "user-agent-data-types": "^0.4.2",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "@effect/schema": "^0.36.5",
    "@sqlite.org/sqlite-wasm": "3.43.1-build1",
    "effect": "2.0.0-next.34",
    "expo": "^49.0.11",
    "expo-sqlite": "~11.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.15"
  },
  "sideEffects": false
}
