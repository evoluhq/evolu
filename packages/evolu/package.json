{
  "name": "evolu",
  "version": "4.1.2",
  "description": "React Hooks library for local-first apps with end-to-end encrypted backup and sync using SQLite and CRDT.",
  "keywords": [
    "evolu",
    "react",
    "hooks",
    "local-first",
    "E2EE",
    "SQLite",
    "CRDT"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./timestamp": {
      "types": "./dist/src/timestamp.d.ts",
      "import": "./dist/src/timestamp.js"
    },
    "./merkleTree": {
      "types": "./dist/src/merkleTree.d.ts",
      "import": "./dist/src/merkleTree.js"
    },
    "./protobuf": {
      "types": "./dist/src/protobuf.d.ts",
      "import": "./dist/src/protobuf.js"
    }
  },
  "files": [
    "dist/sqlite/**",
    "dist/src/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && rm -rf ./README.md && tsc && cp ../../README.md ./ && cp ./sqlite/sqlite3.wasm ./dist/sqlite/",
    "build-sqlite": "scripts/build-sqlite.sh",
    "lint": "TIMING=1 eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\"",
    "proto": "pnpm protoc --ts_out ./src --proto_path protos protos/protobuf.proto --ts_opt eslint_disable && pnpm format"
  },
  "dependencies": {
    "@effect/data": "^0.10.3",
    "@noble/hashes": "^1.3.0",
    "@protobuf-ts/runtime": "^2.8.3",
    "@scure/bip39": "^1.2.0",
    "fp-ts": "^2.13.1",
    "kysely": "~0.24.2",
    "micro-aes-gcm": "~0.4.0",
    "murmurhash": "^2.0.1",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@effect/schema": "~0.10.0",
    "@evolu/tsconfig": "workspace:0.0.2",
    "@protobuf-ts/plugin": "^2.8.3",
    "@protobuf-ts/protoc": "^2.8.3",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/web-locks-api": "^0.0.2",
    "array-shuffle": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-evolu": "workspace:0.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "user-agent-data-types": "^0.3.1",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@effect/schema": "~0.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.15"
  },
  "sideEffects": false
}
