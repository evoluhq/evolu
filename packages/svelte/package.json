{
  "name": "@evolu/svelte",
  "version": "2.0.0-preview.0",
  "description": "Evolu for Svelte",
  "keywords": [
    "evolu",
    "svelte"
  ],
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "scripts": {
    "dev": "tsc --watch",
    "build": "shx rm -rf dist && tsc && pnpm run package",
    "preview": "vite preview",
    "package": "svelte-package",
    "prepublishOnly": "pnpm run package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "shx rm -rf .turbo .svelte-kit node_modules dist"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [],
  "svelte": "./dist/index.svelte.js",
  "types": "./dist/index.svelte.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.svelte.d.ts",
      "svelte": "./dist/index.svelte.js"
    }
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "@evolu/web": "workspace:*",
    "@sveltejs/package": "^2.3.11",
    "@tsconfig/svelte": "^5.0.4",
    "svelte": "^5.33.17",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@evolu/common": "^7.0.0-preview.0",
    "@evolu/web": "^2.0.0-preview.0",
    "svelte": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
