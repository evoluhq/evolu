{
  "name": "@evolu/react-native",
  "version": "14.1.1",
  "description": "Evolu for React Native and Expo",
  "keywords": [
    "evolu",
    "expo",
    "expo-sqlite",
    "op-sqlite",
    "react-native",
    "react-hooks"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "react-native": "./dist/index.js"
    },
    "./expo-sqlite": {
      "types": "./dist/exports/expo-sqlite.d.ts",
      "default": "./dist/exports/expo-sqlite.js",
      "react-native": "./dist/exports/expo-sqlite.js",
      "browser": "./dist/web.js"
    },
    "./expo-op-sqlite": {
      "types": "./dist/exports/expo-op-sqlite.d.ts",
      "default": "./dist/exports/expo-op-sqlite.js",
      "react-native": "./dist/exports/expo-op-sqlite.js",
      "browser": "./dist/web.js"
    },
    "./bare-op-sqlite": {
      "types": "./dist/exports/bare-op-sqlite.d.ts",
      "default": "./dist/exports/bare-op-sqlite.js",
      "react-native": "./dist/exports/bare-op-sqlite.js",
      "browser": "./dist/web.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "expo-sqlite": [
        "./dist/exports/expo-sqlite.d.ts"
      ],
      "expo-op-sqlite": [
        "./dist/exports/expo-op-sqlite.d.ts"
      ],
      "bare-op-sqlite": [
        "./dist/exports/bare-op-sqlite.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "shx rm -rf dist && tsc",
    "clean": "shx rm -rf .turbo node_modules dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/react": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "@op-engineering/op-sqlite": "^15.0.3",
    "expo": "^54.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-sqlite": "~16.0.8",
    "react-native": "^0.81.4",
    "react-native-nitro-modules": "^0.31.2",
    "react-native-sensitive-info": "^6.0.0-rc.8",
    "react-native-svg": "^15.14.0",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.4"
  },
  "peerDependencies": {
    "@evolu/common": "^7.2.1",
    "@evolu/react": "^10.2.1",
    "@op-engineering/op-sqlite": ">=12",
    "expo": ">=54",
    "expo-secure-store": ">=15",
    "expo-sqlite": ">=16",
    "react-native": ">=0.81",
    "react-native-nitro-modules": ">=0.31",
    "react-native-sensitive-info": ">=6",
    "react-native-svg": ">=15"
  },
  "peerDependenciesMeta": {
    "@op-engineering/op-sqlite": {
      "optional": true
    },
    "expo": {
      "optional": true
    },
    "expo-sqlite": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    },
    "react-native-nitro-modules": {
      "optional": true
    },
    "react-native-sensitive-info": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "sideEffects": []
}
